{
	
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"patterns": ["image-and-text","personal-bio","image-section"],
	"settings": {
		"appearanceTools": true,
		"layout": {
			"contentSize": "var(--wp--custom--layout--content-size)",
			"wideSize": "var(--wp--custom--layout--wide-size)"
		},
		"color": {
			"palette": [
				{
					"slug": "background",
					"color": "#F5E8E4",
					"name": "Background"
				},
				{
					"slug": "foreground",
					"color": "#2b1c25",
					"name": "Foreground"
				},
				{
					"slug": "primary",
					"color": "#D1512D",
					"name": "Primary"
				},
				{
					"slug": "secondary",
					"color": "#F5C7A9",
					"name": "Secondary"
				},
				{
					"slug": "tertiary",
					"color": "#411530",
					"name": "Tertiary"
				},
				{
					"slug": "transparent",
					"color": "transparent",
					"name": "Transparent"
				}
			],
			"gradients": [
				{
					"slug": "vertical-background-primary",
					"gradient": "linear-gradient(90deg, var(--wp--preset--color--background) 30%, var(--wp--preset--color--primary) 30%)",
					"name": "Vertical background to primary"
				},
				{
					"slug": "vertical-background-secondary",
					"gradient": "linear-gradient(90deg, var(--wp--preset--color--background) 30%, var(--wp--preset--color--secondary) 30%)",
					"name": "Vertical background to secondary"
				},
				{
					"slug": "vertical-primary-secondary",
					"gradient": "linear-gradient(90deg, var(--wp--preset--color--primary) 30%, var(--wp--preset--color--secondary) 30%)",
					"name": "Vertical primary to secondary"
				},
				{
					"slug": "diagonal-background-primary",
					"gradient": "linear-gradient(to bottom right, var(--wp--preset--color--background) 50%, var(--wp--preset--color--primary) 50%)",
					"name": "Diagonal background to primary"
				},
				{
					"slug": "diagonal-background-secondary",
					"gradient": "linear-gradient(to bottom right, var(--wp--preset--color--background) 50%, var(--wp--preset--color--secondary) 50%)",
					"name": "Diagonal background to secondary"
				},
				{
					"slug": "diagonal-primary-secondary",
					"gradient": "linear-gradient(to bottom right, var(--wp--preset--color--primary) 50%, var(--wp--preset--color--secondary) 50%)",
					"name": "Diagonal primary to secondary"
				}


			],
			"duotone": [
				{
					"colors": [ "#F5C7A9", "#D1512D" ],
					"slug": "secondary-primary",
					"name": "Secondary and primary"
				},
				{
					"colors": [ "#2b1c25", "#D1512D" ],
					"slug": "foreground-primary",
					"name": "Foreground and primary"
				},
				{
					"colors": [ "#D1512D", "#F5E8E4" ],
					"slug": "primary-background",
					"name": "Primary and background"
				}
			]
		},
		"spacing": {
			"units": [ "px", "em", "rem", "vh", "vw", "%" ],
			"blockGap": true
		},
		"typography": {
			"dropCap": true,
			"customFontSize": true,
			"fontFamilies": [
				{
					"name": "System",
					"slug": "system",
					"fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif"
				},
				{
					"name": "Serif",
					"slug": "serif",
					"fontFamily": "\"Times New Roman\",\"New York\",Times,\"Noto Serif\",serif"
				},
				{
					"name": "Monospace",
					"slug": "monospace",
					"fontFamily": "Consolas,Menlo,Monaco,\"SF Mono\",\"DejaVu Sans Mono\",\"Roboto Mono\",\"Courier New\",Courier,monospace"
				},
				{
					"name": "Poppins",
					"slug": "poppins",
					"fontFamily": "Poppins,-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif"
				},
				{
					"name": "Outfit",
					"slug": "outfit",
					"fontFamily": "Outfit,-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif"
				},
				{
					"name": "Montserrat",
					"slug": "montserrat",
					"fontFamily": "Montserrat,-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif"
				},
				{
					"name": "Ibarra Real Nova",
					"slug": "ibarra-real-nova",
					"fontFamily": "\"Ibarra Real Nova\", \"Times New Roman\",\"New York\",Times,\"Noto Serif\",serif"
				},
				{
					"name": "Lora",
					"slug": "lora",
					"fontFamily": "Lora,\"Times New Roman\",\"New York\",Times,\"Noto Serif\",serif",
					"fontFace": [
						{
							"fontFamily": "Lora",
							"fontWeight": "200 900",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/lora-v23-latin-ext_latin-regular.woff2" ]
						},
						{
							"fontFamily": "Lora",
							"fontWeight": "200 900",
							"fontStyle": "italic",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/lora-v23-latin-ext_latin-italic.woff2" ]
						}
					]
				}
			],
			"fontSizes":[
				{
					"slug": "small",
					"size": "0.95rem",
					"name": "Small"
				},
				{
					"slug": "normal",
					"size": "clamp(1.2rem, calc(1rem + 0.5vw), 1.4rem) !important",
					"name": "Normal"
				},
				{
					"slug": "large",
					"size": "clamp(1.75rem, calc(1rem + 1.5vw), 2rem)",
					"name": "Large"
				},
				{
					"slug": "x-large",
					"size": "clamp(2rem, calc(1rem + 2vw), 2.75rem)",
					"name": "XL"
				},
				{
					"slug": "big",
					"size": "clamp(3rem, calc(1rem + 4vw), 6rem)",
					"name": "Big"
				},
				{
					"slug": "enormous",
					"size": "clamp(6rem, calc(1rem + 12vw), 15rem)",
					"name": "Enormous"
				}
			],
			"textTransform": true
		},
		"custom": {
			"h-spacing": "2.375rem",
			"v-spacing": "clamp(1.25rem,2.2vw,2rem)",
			"text-shadow": "0 2px 8px rgba(0, 0, 0, 0.2)",
			"button-shadow": "0 8px 8px 0 rgba(0, 0, 0, 0.2)",
			"small-button-shadow": "0px 5px 0px 0 rgba(0, 0, 0, 1)",
			"button-highlight": "0 0 0 #fff",
			"button-border-color": "rgba(0, 0, 0, 0.5)",
			"button-filter": "contrast(140%)",
			"input-border-radius": "4px",
			"input-padding": ".5rem 1.2rem",
			"layout":{
				"contentSize": "min( calc(100vw - 40px), 780px)",
				"wideSize": "min( calc(100vw - 40px), 1280px)"
			}
		}
	},
	"styles": {
		"spacing":{
			"blockGap": "var(--wp--custom--v-spacing)",
			"margin": {
				"top": "0px",
				"right": "0px",
				"bottom": "0px",
				"left": "0px"
			}
		},
		"color": {
			"background": "var(--wp--preset--color--background)",
			"text": "var(--wp--preset--color--foreground)"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--outfit)",
			"fontSize": "var(--wp--preset--font-size--normal)",
			"lineHeight": "1.4"
		},
		"elements": {
			"link": {
				"color": {
					"text": "var(--wp--preset--color--foreground)"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--big)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			}
		},
		"blocks": {
			"core/button": {
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--background)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--normal)"
				},
				"border": {
					"radius": "8px",
					"color": "inherit"
				},
				"spacing": {
					"padding": {
						"top": "calc(var(--wp--custom--v-spacing) * 0.5)",
						"right": "var(--wp--custom--v-spacing)",
						"bottom": "calc(var(--wp--custom--v-spacing) * 0.5)",
						"left": "var(--wp--custom--v-spacing)"
					}
				}
			},
			"core/code": {
				"color": {
					"text": "var(--wp--preset--color--foreground)"
				}
			},
			"core/post-navigation-link": {
				"spacing": {
					"margin": {
						"top": "calc(var(--wp--custom--v-spacing) * 3)",
						"bottom": "var(--wp--custom--v-spacing)"
					}
				}
			},
			"core/query-pagination": {
				"spacing": {
					"margin": {
						"top": "calc(var(--wp--custom--v-spacing) * 3) !important"
					}
				}
			},
			"core/post-author": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				},
				"color": {
					"text": "var(--wp--preset--color--primary)"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--primary)"
						}
					}
				}
			},
			"core/post-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--extra-small)"
				},
				"color": {
					"text": "var(--wp--preset--color--primary)"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--primary)"
						}
					}
				}
			},
			"core/post-terms": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--extra-small)"
				},
				"color": {
					"text": "var(--wp--preset--color--primary)"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--primary)"
						}
					}
				}
			},
			"core/latest-comments": {
				"spacing": {
					"padding": {
						"left": "0px"
					}
				}
			}
		}
	},
	"templateParts": [
		{
			"name": "header",
			"title": "Header",
			"area": "header"
		},
		{
			"name": "header-transparent",
			"title": "Transparent header",
			"area": "header"
		},
		{
			"name": "narrow-header",
			"title": "Narrow Header",
			"area": "header"
		},
		{
			"name": "footer",
			"title": "Footer",
			"area": "footer"
		},
		{
			"name": "narrow-footer",
			"title": "Narrow Footer",
			"area": "footer"
		},
		{
			"name": "footer-large",
			"title": "Large footer",
			"area": "footer"
		},
		{
			"name": "sidebar",
			"title": "Sidebar",
			"area": "uncategorized"
		}
	],
	"customTemplates": [
		{
			"name": "no-title",
			"title": "No title",
			"postTypes": [ "post","page" ]
		},
		{
			"name": "page-featured-image-header",
			"title": "Featured image header",
			"postTypes": [ "post","page" ]
		},
		{
			"name": "sidebar-page-template",
			"title": "Page with sidebar",
			"postTypes": [ "page" ]
		},
		{
			"name": "sidebar-post-template",
			"title": "Post with sidebar",
			"postTypes": [ "post" ]
		},
		{
			"name": "single-product",
			"title": "WooCommerce single product (Requires WooCommerce)"
		},
		{
			"name": "archive-product",
			"title": "WooCommerce product archive (Requires WooCommerce)"
		},
		{
			"name": "page-checkout",
			"title": "WooCommerce Checkout (Requires WooCommerce Blocks)"
		},
		{
			"name": "page-cart",
			"title": "WooCommerce Cart (Requires WooCommerce Blocks)"
		}
	]
}
